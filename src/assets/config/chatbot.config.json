{
  "organization": {
    "name": "Organization Name",
    "logo": "assets/images/logominsait_normal.png",
    "logoDark": "assets/images/logominsait_white.png",
    "favicon": "assets/images/favicon.png",
    "url": "https://www.minsait.com"
  },

  "apiConnection": {
    "url": "http://127.0.0.1:5000/hello",
    "promptUrl": "http://127.0.0.1:5000/model/prompt",
    "feedbackUrl": "http://127.0.0.1:5000/model/prompt/vote",
    "modelName": "model_name",

    "request": {
      "promptDTO": {
        "id": "id",
        "role": "role",
        "content": "question",
        "user": "user_groups",
        "project": "project_name",
        "modelName": "model_name"
      },
      "feedbackDTO": {
        "promptId": "prompt_answer_id",
        "feedbackStyle": "feedback_style",
        "rating": "vote_type"
      }
    },

    "response": {
      "promptDTO": {
        "id": "id",
        "role": "role",
        "content": "content",
        "timestamp": "created_at"
      },
      "promptAnswerDTO": {
        "id": "id",
        "role": "role",
        "promptId": "prompt_id",
        "content": "content",
        "timestamp": "created_at"
      },
      "feedbackDTO": {
        "promptId": "prompt_answer_id",
        "rating": "rating",
        "timestamp": "created_at"
      }
    }
  },
  
  "layout": {
    "chatbotPrompt": {
      "allowCopy": true
    },
    "chatbotPromptAnswer": {
      "promptFeedback": {
        "active": true,
        "style": "rating",
        "maxRating": 5
      },
      "promptCopy": {
        "active": true
      }
    },
    "sidePanel": {
      "active": true,
      "position": "left",
      "resizeable": true,
      "width": "300px",
      "minWidth": "200px",
      "maxWidth": "400px"
    }
  },

  "colors": {
    "main_bg": "#FFFEFE",
    "main_primary": "#E6E7E6",
    "main_secondary": "#1B3A49",
    "main_primary_light": "#F3F3F2",
    "main_secondary_light": "#819198",
    "main_hl_primary": "#FCE1D3",
    "main_hl_primary_dark": "#F6B08D",
    "main_hl_secondary": "#78C2B4",
    "main_hl_secondary_dark": "#4A7E7F",

    "dark_bg": "#262730",
    "dark_primary": "#0e1118",
    "dark_secondary": "#a0a6b7",
    "dark_primary_light": "#1A1C24",
    "dark_secondary_light": "#575C68",
    "dark_hl_primary": "#FCE1D3",
    "dark_hl_primary_dark": "#F6B08D",
    "dark_hl_secondary": "#78C2B4",
    "dark_hl_secondary_dark": "#4A7E7F",

    "success": "#28a745",
    "info": "#17a2b8",
    "warning": "#ffc107",
    "danger": "#dc3545"
  }
}
